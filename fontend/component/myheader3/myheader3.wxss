
.nav{
  position: fixed;
  width: 100%;
  z-index: 9;
}

.navbar{
    /* position: fixed; */
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    z-index: 9;
}


.back-back{
  width: 87px;
  height: 31px;
  
  border-radius: 100px;
  margin-left: 8px;
  display: flex;
  justify-content: space-evenly;
  
}
.backgr{
  position: absolute;
  width: 87px;
  height: 30px;
  border-radius: 100px;
  background: rgba(0,0,0,1);
  border: 0.5px solid #fefefe;
  opacity: 0.16;
}

.back-icon, .home-icon{
    width: 28px;
    height: 100%;
    position: relative;    
    transform: translateY(-50%);    
    top: 50%;    
    display: flex;
    }

.line{
  width: 1px;
  height: 16px;
  transform: translateY(-50%);   
  top: 50%;
  background-color: #ffffff;
  opacity: 0.3;
  /* left : 51.5px; */
  position: relative;
}

.back-icon image{    
    width: 18px;    
    height: 18px;    
    margin: auto;
    
}

.home-icon image{    
    width: 20px;    
    height: 20px;    
    margin: auto;
}
.nav-title, .nav-icon{    
    position: relative;   
    justify-content: center;
    align-items: center; 
    /* transform: translate(-50%, -50%);     */
    left: -42%;    
    /* top: 50%;     */
    font-size: 0;    
    font-weight: normal;
}
.sw-bg{
  width: 100%;
  height: 0px;
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 16px;
  color: #ffffff;
}