
.nav{
  position: fixed;
  width: 100%;
  z-index: 9;
}

.navbar{
    /* position: fixed; */
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    z-index: 9;
}


.search-bar{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width: 400rpx;
  height: 26px;
  border-radius: 100rpx;
  background-color: #f8f8f8;
  /* opacity: 0.7; */
  margin-left: 8px;
}
.search-bar-pic{
  width: 16px;
  height: 16px;
  margin-left: 10px;
}
.search-bar-text{
  font-size: 13px;
  margin-left: 8px;
  color: #979797;
  letter-spacing: 1.5px;
  line-height: 30px;
}